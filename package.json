{
  "name": "mongoose-api",
  "version": "1.0.0",
  "description": "Learning node, express and mongoDB",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=development nodemon --exec babel-node src/server.js",
    "start:prod": "NODE_ENV=production nodemon dist/server.js",
    "build": "NODE_ENV=development webpack",
    "serve": "node dist/lib.node.js",
    "debug": "ndb server.js",
    "init-data": "nodemon --exec babel-node dev-data/index.js  --import",
    "delete-data": "nodemon --exec babel-node dev-data/index.js  --delete"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "mongoose": "^5.5.2",
    "morgan": "^1.9.1",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack-cli": "^3.3.11",
    "babel-loader": "^8.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
